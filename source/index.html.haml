%section.quienes-somos#quienes-somos
	.inner
		%h2 <span class="proplac-red">Q</span>UIÉNES SOMOS?
		%div{"data-alt" => "", "data-picture" => "", "class" => "media-block"}
			%div{"data-src" => "assets/img/nosotros/phone/nosotros.jpg"}
